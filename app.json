{
  "expo": {
    "name": "MyArabicLearner",
    "slug": "MyArabicLearner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "fonts": [
      {
        "asset": "./assets/fonts/ArefRuqaa-Regular.ttf",
        "family": "ArefRuqaa"
      },
      {
        "asset": "./assets/fonts/ArefRuqaa-Bold.ttf",
        "family": "ArefRuqaa-Bold"
      },
      {
        "asset": "./assets/fonts/NotoKufiArabic-Regular.ttf",
        "family": "NotoKufiArabic"
      },
      {
        "asset": "./assets/fonts/NotoKufiArabic-Bold.ttf",
        "family": "NotoKufiArabic-Bold"
      },
      {
        "asset": "./assets/fonts/ClashGrotesk-Light.ttf",
        "family": "ClashGrotesk-Light"
      },
      {
        "asset": "./assets/fonts/ClashGrotesk-Medium.ttf",
        "family": "ClashGrotesk"
      }
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.musaq.MyArabicLearner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c1cde764-15e1-451f-b703-5ad986d574f7"
      }
    },
    "owner": "musa-q"
  }
}